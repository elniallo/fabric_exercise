{
  "name": "fabric_exercise",
  "version": "1.0.0",
  "description": "Fabric Chaincode for Blockchain Donations",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "nyc -r lcov -e .ts -x \"*.spec.ts\" jasmine-ts --config=jasmine.json && nyc report",
    "docs": "rm -rf docs && typedoc --out ./docs/ ./src --theme markdown --readme none"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elniallo/fabric_exercise.git"
  },
  "author": "Niall Moore <niallmoore22@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elniallo/fabric_exercise/issues"
  },
  "homepage": "https://github.com/elniallo/fabric_exercise#readme",
  "devDependencies": {
    "@types/jasmine": "^3.3.9",
    "jasmine": "^3.3.1",
    "jasmine-ts": "^0.3.0",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.1.27",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "fabric-contract-api": "^1.4.0",
    "fabric-shim": "^1.4.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage/**",
      "dist/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  }
}
